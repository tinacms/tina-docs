{
  "name": "tina-docs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "tinacms dev -c \"next dev --turbopack\"",
    "build": "tinacms build && next build --turbopack && npx pagefind --site out",
    "build-local": "tinacms build --local --skip-indexing --skip-cloud-checks && next build && npx pagefind --site out",
    "start": "tinacms build && next start",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check src/"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@monaco-editor/react": "^4.7.0",
    "fast-glob": "^3.3.3",
    "lodash": "^4.17.21",
    "mermaid": "^11.6.0",
    "monaco-editor": "^0.52.2",
    "next": "^15.3.0",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-animate-height": "^3.2.3",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.3",
    "tinacms": "^2.6.1",
    "typescript": "5.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tailwindcss/postcss": "^4.1.4",
    "@tinacms/cli": "^1.8.1",
    "@types/node": "^22.10.1",
    "autoprefixer": "^10.4.17",
    "pagefind": "^1.3.0",
    "postcss": "^8.4.35",
    "postcss-preset-env": "^9.3.0",
    "tailwindcss": "^4.1.0"
  },
  "packageManager": "pnpm@9.15.2"
}
